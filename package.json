{
  "name": "webpack-app",
  "version": "1.0.0",
  "description": "webpack project",
  "main": "index.js",
  "scripts": {
    "build": "webpack index.js --output bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-css/webpack.config.js",
    "build:multiple:js": "webpack --config ./multiple-entry-point/webpack.config.js",
    "build:dev": "webpack --config ./dev-server/webpack.config.js --watch",
    "build:dev:server": "webpack-dev-server --config ./dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./file-loader-video/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:sass": "webpack --config ./sass-loader/webpack.config.js",
    "build:stylus": "webpack --config ./stylus-loader/webpack.config.js",
    "build:less": "webpack --config ./less-loader/webpack.config.js",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js",
    "build:duplication": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:vendor": "webpack --config ./vendor/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:dll:src": "webpack --config ./dll/webpack.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js",
    "build:invie": "webpack-dev-server --config ./invie/webpack.dev.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Jugo",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "json-loader": "^0.5.7",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.4",
    "nib": "^1.1.2",
    "node-sass": "^4.10.0",
    "postcss": "^7.0.5",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "rupture": "^0.7.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
